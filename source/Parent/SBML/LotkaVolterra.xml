<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level2/version4" level="2" version="4">
  <model id="MPModel">
    <notes>
      <body xmlns="http://www.w3.org/1999/xhtml">
        <p>
          <h2>
            <center>Lotka Volterra</center>
          </h2>
        </p>
        <p>Description: The Lotka Volterra pred-prey model.</p>
        <p>This model has been automatically generated by the MetaPlab SBML plugin.<br/>
The original model has <b>time unit = 1.0 seconds</b> and <b>time resolution = 1.0</b>
        <br/>
In order to obtain the same dynamics of the original model, the user should calculate it by means of the <b>Euler algorithm with dT = 1.0</b> (which corresponds to 1.0 seconds)</p>
      </body>
    </notes>
    <listOfCompartments>
      <compartment id="skin" name="skin membrane" size="1" units="volume"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="Prey" name="Prey" compartment="skin" initialConcentration="900" substanceUnits="substance" boundaryCondition="true" constant="true"/>
      <species id="Pred" name="Predators" compartment="skin" initialConcentration="900" substanceUnits="substance" boundaryCondition="true" constant="true"/>
    </listOfSpecies>
    <listOfParameters>
      <parameter id="I" name="Inertia" value="5" units="dimensionless"/>
      <parameter id="K0" name="Const0" value="0.03" units="dimensionless"/>
      <parameter id="K1" name="Const1" value="4e-05" units="dimensionless"/>
      <parameter id="K2" name="Const2" value="0.03" units="dimensionless"/>
      <parameter id="R0React" name="Reactivity of R0" value="0.03" units="dimensionless" constant="false"/>
      <parameter id="R1React" name="Reactivity of R1" value="0.036" units="dimensionless" constant="false"/>
      <parameter id="PreyPress" name="Prey pressure" value="0.066" units="dimensionless" constant="false"/>
      <parameter id="R2React" name="Reactivity of R2" value="0.03" units="dimensionless" constant="false"/>
      <parameter id="PredPress" name="Predator pressure" value="0.066" units="dimensionless" constant="false"/>
    </listOfParameters>
    <listOfRules>
      <assignmentRule metaid="R0ReactEvolution" variable="R0React">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <ci> K0 </ci>
        </math>
      </assignmentRule>
      <assignmentRule metaid="R1ReactEvolution" variable="R1React">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <ci> K1 </ci>
            <piecewise>
              <piece>
                <ci> Prey </ci>
                <apply>
                  <gt/>
                  <ci> Prey </ci>
                  <ci> Pred </ci>
                </apply>
              </piece>
              <otherwise>
                <ci> Pred </ci>
              </otherwise>
            </piecewise>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule metaid="PreyPressEvolution" variable="PreyPress">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <plus/>
            <ci> R0React </ci>
            <ci> R1React </ci>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule metaid="R2ReactEvolution" variable="R2React">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <ci> K2 </ci>
        </math>
      </assignmentRule>
      <assignmentRule metaid="PredPressEvolution" variable="PredPress">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <plus/>
            <ci> R1React </ci>
            <ci> R2React </ci>
          </apply>
        </math>
      </assignmentRule>
    </listOfRules>
    <listOfReactions>
      <reaction id="R0" reversible="false" fast="false">
        <listOfReactants>
          <speciesReference species="Prey"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Prey" stoichiometry="2"/>
        </listOfProducts>
        <kineticLaw>
          <notes>
            <body xmlns="http://www.w3.org/1999/xhtml">
              <p>Flux name: Fr0</p>
            </body>
          </notes>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <divide/>
              <apply>
                <times/>
                <ci> R0React </ci>
                <ci> Prey </ci>
              </apply>
              <apply>
                <plus/>
                <ci> I </ci>
                <ci> PreyPress </ci>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="R1" reversible="false" fast="false">
        <listOfReactants>
          <speciesReference species="Pred"/>
          <speciesReference species="Prey"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Pred" stoichiometry="2"/>
        </listOfProducts>
        <kineticLaw>
          <notes>
            <body xmlns="http://www.w3.org/1999/xhtml">
              <p>Flux name: Fr1</p>
            </body>
          </notes>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <piecewise>
              <piece>
                <apply>
                  <divide/>
                  <apply>
                    <times/>
                    <ci> R1React </ci>
                    <ci> Prey </ci>
                  </apply>
                  <apply>
                    <plus/>
                    <ci> I </ci>
                    <ci> PreyPress </ci>
                  </apply>
                </apply>
                <apply>
                  <lt/>
                  <apply>
                    <divide/>
                    <apply>
                      <times/>
                      <ci> R1React </ci>
                      <ci> Prey </ci>
                    </apply>
                    <apply>
                      <plus/>
                      <ci> I </ci>
                      <ci> PreyPress </ci>
                    </apply>
                  </apply>
                  <apply>
                    <divide/>
                    <apply>
                      <times/>
                      <ci> R1React </ci>
                      <ci> Pred </ci>
                    </apply>
                    <apply>
                      <plus/>
                      <ci> I </ci>
                      <ci> PredPress </ci>
                    </apply>
                  </apply>
                </apply>
              </piece>
              <otherwise>
                <apply>
                  <divide/>
                  <apply>
                    <times/>
                    <ci> R1React </ci>
                    <ci> Pred </ci>
                  </apply>
                  <apply>
                    <plus/>
                    <ci> I </ci>
                    <ci> PredPress </ci>
                  </apply>
                </apply>
              </otherwise>
            </piecewise>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="R2" reversible="false" fast="false">
        <listOfReactants>
          <speciesReference species="Pred"/>
        </listOfReactants>
        <kineticLaw>
          <notes>
            <body xmlns="http://www.w3.org/1999/xhtml">
              <p>Flux name: Fr2</p>
            </body>
          </notes>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <divide/>
              <apply>
                <times/>
                <ci> R2React </ci>
                <ci> Pred </ci>
              </apply>
              <apply>
                <plus/>
                <ci> I </ci>
                <ci> PredPress </ci>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
