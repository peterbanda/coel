          </body>
        </notes>
      </parameter>
    </listOfParameters>
    <listOfRules>
      <assignmentRule metaid="ClkEvolution" variable="Clk">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <piecewise>
            <piece>
              <cn> 0 </cn>
              <apply>
                <leq/>
                <ci> S </ci>
                <ci> K </ci>
              </apply>
            </piece>
            <otherwise>
              <piecewise>
                <piece>
                  <cn> 1 </cn>
                  <apply>
                    <geq/>
                    <ci> S </ci>
                    <apply>
                      <minus/>
                      <cn> 1 </cn>
                      <ci> K </ci>
                    </apply>
                  </apply>
                </piece>
                <otherwise>
                  <ci> oldClk </ci>
                </otherwise>
              </piecewise>
            </otherwise>
          </piecewise>
        </math>
      </assignmentRule>
      <assignmentRule metaid="SignEvolution" variable="Sign">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <piecewise>
            <piece>
              <apply>
                <not/>
                <ci> oldSign </ci>
              </apply>
              <apply>
                <geq/>
                <ci> Tmp </ci>
                <apply>
                  <minus/>
                  <cn> 1 </cn>
                  <ci> K </ci>
                </apply>
              </apply>
            </piece>
            <otherwise>
              <ci> oldSign </ci>
            </otherwise>
          </piecewise>
        </math>
      </assignmentRule>
      <assignmentRule metaid="SinusEvolution" variable="Sinus">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <plus/>
            <apply>
              <times/>
              <apply>
                <eq/>
                <ci> Sign </ci>
                <cn> 1 </cn>
              </apply>
              <ci> S </ci>
            </apply>
            <apply>
              <times/>
              <apply>
                <eq/>
                <ci> Sign </ci>
                <cn> 0 </cn>
              </apply>
              <apply>
                <minus/>
                <ci> S </ci>
              </apply>
            </apply>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule metaid="oldClkEvolution" variable="oldClk">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <piecewise>
            <piece>
              <ci> absOldClk </ci>
              <apply>
                <eq/>
                <ci> signOldClk </ci>
                <cn> 1 </cn>
              </apply>
            </piece>
            <otherwise>
              <apply>
                <minus/>
                <ci> absOldClk </ci>
              </apply>
            </otherwise>
          </piecewise>
        </math>
      </assignmentRule>
      <assignmentRule metaid="oldSignEvolution" variable="oldSign">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
