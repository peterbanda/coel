<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level2/version4" level="2" version="4">
  <model id="MPModel">
    <notes>
      <body xmlns="http://www.w3.org/1999/xhtml">
        <p>
          <h2>
            <center>An MP solver for the 3x+1 problem</center>
          </h2>
        </p>
        <p>Description: An MP solver which uses an MP Collatz dynamical system</p>
        <p>This model has been automatically generated by the MetaPlab SBML plugin.<br/>
The original model has <b>time unit = 1.0 seconds</b> and <b>time resolution = 1.0</b>
        <br/>
In order to obtain the same dynamics of the original model, the user should calculate it by means of the <b>Euler algorithm with dT = 1.0</b> (which corresponds to 1.0 seconds)</p>
      </body>
    </notes>
    <listOfCompartments>
      <compartment id="skin" name="skin membrane" size="1" units="volume"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="Nc" name="Substance" compartment="skin" initialConcentration="1" substanceUnits="substance" boundaryCondition="true" constant="true"/>
      <species id="absOldNumber" name="Old reference for the absolute value of Number" compartment="skin" initialConcentration="1" substanceUnits="substance" boundaryCondition="true" constant="true"/>
      <species id="signOldNumber" name="Old reference for the sign of Number" compartment="skin" initialConcentration="1" substanceUnits="substance" boundaryCondition="true" constant="true"/>
    </listOfSpecies>
    <listOfParameters>
      <parameter id="Number" name="Parameter" value="1" units="dimensionless" constant="false"/>
      <parameter id="oldNumber" name="Old reference for Number" value="1" units="dimensionless" constant="false">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml">
            <p>Additional information: Old reference for Number</p>
          </body>
        </notes>
      </parameter>
    </listOfParameters>
    <listOfRules>
      <assignmentRule metaid="NumberEvolution" variable="Number">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <piecewise>
            <piece>
              <apply>
                <plus/>
                <ci> oldNumber </ci>
                <cn> 1 </cn>
              </apply>
              <apply>
                <eq/>
                <ci> Nc </ci>
                <cn> 1 </cn>
              </apply>
            </piece>
            <otherwise>
              <ci> oldNumber </ci>
            </otherwise>
          </piecewise>
        </math>
      </assignmentRule>
      <assignmentRule metaid="oldNumberEvolution" variable="oldNumber">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <piecewise>
            <piece>
              <ci> absOldNumber </ci>
              <apply>
                <eq/>
                <ci> signOldNumber </ci>
                <cn> 1 </cn>
              </apply>
            </piece>
            <otherwise>
              <apply>
                <minus/>
                <ci> absOldNumber </ci>
              </apply>
            </otherwise>
          </piecewise>
        </math>
      </assignmentRule>
    </listOfRules>
    <listOfReactions>
      <reaction id="Ins" reversible="false" fast="false">
        <listOfProducts>
          <speciesReference species="Nc"/>
        </listOfProducts>
        <kineticLaw>
          <notes>
            <body xmlns="http://www.w3.org/1999/xhtml">
              <p>Flux name: FIns</p>
            </body>
          </notes>
        </kineticLaw>
      </reaction>
      <reaction id="Del" reversible="false" fast="false">
        <listOfReactants>
          <speciesReference species="Nc"/>
        </listOfReactants>
        <kineticLaw>
          <notes>
            <body xmlns="http://www.w3.org/1999/xhtml">
              <p>Flux name: FDel</p>
            </body>
          </notes>
        </kineticLaw>
      </reaction>
      <reaction id="Inc" reversible="false" fast="false">
        <listOfProducts>
          <speciesReference species="Nc"/>
        </listOfProducts>
        <kineticLaw>
          <notes>
            <body xmlns="http://www.w3.org/1999/xhtml">
              <p>Flux name: FInc</p>
            </body>
          </notes>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <apply>
                <eq/>
                <ci> Nc </ci>
                <cn> 1 </cn>
              </apply>
              <ci> Number </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="RIabsOldNumber" reversible="false" fast="false">
        <listOfProducts>
          <speciesReference species="absOldNumber"/>
        </listOfProducts>
        <kineticLaw>
          <notes>
            <body xmlns="http://www.w3.org/1999/xhtml">
              <p>Flux name: FIabsOldNumber</p>
              <p>Additional information: Input flux for absOldNumber</p>
            </body>
          </notes>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <abs/>
              <ci> Number </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="ROabsOldNumber" reversible="false" fast="false">
        <listOfReactants>
          <speciesReference species="absOldNumber"/>
        </listOfReactants>
        <kineticLaw>
          <notes>
            <body xmlns="http://www.w3.org/1999/xhtml">
              <p>Flux name: FOabsOldNumber</p>
              <p>Additional information: Output flux for absOldNumber</p>
            </body>
          </notes>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <ci> absOldNumber </ci>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="RIsignOldNumber" reversible="false" fast="false">
        <listOfProducts>
          <speciesReference species="signOldNumber"/>
        </listOfProducts>
        <kineticLaw>
          <notes>
            <body xmlns="http://www.w3.org/1999/xhtml">
              <p>Flux name: FIsignOldNumber</p>
              <p>Additional information: Input flux for signOldNumber</p>
            </body>
          </notes>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <piecewise>
              <piece>
                <cn> 1 </cn>
                <apply>
                  <geq/>
                  <ci> Number </ci>
                  <cn> 0 </cn>
                </apply>
              </piece>
              <otherwise>
                <cn> 0 </cn>
              </otherwise>
            </piecewise>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="ROsignOldNumber" reversible="false" fast="false">
        <listOfReactants>
          <speciesReference species="signOldNumber"/>
        </listOfReactants>
        <kineticLaw>
          <notes>
            <body xmlns="http://www.w3.org/1999/xhtml">
              <p>Flux name: FOsignOldNumber</p>
              <p>Additional information: Output flux for signOldNumber</p>
            </body>
          </notes>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <ci> signOldNumber </ci>
          </math>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
